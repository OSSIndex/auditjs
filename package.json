{
  "name": "auditjs",
  "version": "4.0.1-alpha.6",
  "description": "Audit dependencies to identify known vulnerabilities and maintenance problems",
  "main": "./bin/index.js",
  "bin": {
    "auditjs": "./bin/index.js"
  },
  "files": [
    "bin/**/*"
  ],
  "preferGlobal": true,
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "build": "tsc -p tsconfig.json",
    "build-dev": "tsc -p tsconfig.development.json",
    "start": "node ./bin/index.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test"
  },
  "keywords": [
    "Audit",
    "Scan",
    "Vulnerability",
    "CPE",
    "CVE",
    "CWE",
    "OSSIndex",
    "Artie",
    "Nexus",
    "Sonatype"
  ],
  "author": "Sonatype Community",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sonatype-nexus-community/auditjs.git"
  },
  "devDependencies": {
    "@types/chai": "4.2.0",
    "@types/chai-as-promised": "7.1.1",
    "@types/mocha": "5.2.7",
    "@types/mock-fs": "^4.10.0",
    "@types/rimraf": "^2.0.3",
    "@types/sinon": "^7.5.1",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "mocha": "6.2.2",
    "mock-fs": "^4.10.4",
    "nock": "11.7.0",
    "rimraf": "^3.0.0",
    "sinon": "^8.0.2",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "@cyclonedx/bom": "^1.0.4",
    "@types/figlet": "^1.2.0",
    "@types/node": "^12.12.17",
    "@types/node-fetch": "^2.5.4",
    "@types/node-persist": "^3.0.0",
    "@types/yargs": "^13.0.3",
    "@types/yarnpkg__lockfile": "^1.1.3",
    "@yarnpkg/lockfile": "^1.1.0",
    "chalk": "^3.0.0",
    "colors": "^1.3.1",
    "figlet": "^1.2.4",
    "node-fetch": "^2.6.0",
    "node-persist": "^3.0.5",
    "ora": "^4.0.3",
    "read-installed": "^4.0.3",
    "winston": "^3.2.1",
    "yargs": "^15.0.2"
  }
}
